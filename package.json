{
  "name": "vue-loader",
  "version": "0.0.1",
  "description": "demo",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env PROT=8000 node build/webpack.dev.config.js",
    "test": "webpack -d --progress --hide-modules --colors --config build/webpack.test.config.js",
    "build": "cross-env webpack -p --progress --hide-modules --colors --config build/webpack.product.config.js",
    "http-push": "cross-env HTTP_PUSH=http-push webpack -p --progress --hide-modules --colors --config build/webpack.product.config.js",
    "http-push-build": "cross-env HTTP_PUSH=build node build/http.push.config.js",
    "test-build": "cross-env HTTP_PUSH=test-build npm run test && node ./build/http.push.tar.gz.js"
  },
  "author": "wanwei",
  "license": "MIT",
  "engines": {
    "node": ">=7.0",
    "npm": ">=3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.13",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-push-webpack-plugin": "^1.0.2",
    "imports-loader": "^0.7.1",
    "jquery": "^3.1.0",
    "log-util": "^1.1.2",
    "node-sass": "^4.5.3",
    "request": "^2.81.0",
    "sass-loader": "^6.0.6",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.19.0",
    "tar.gz": "^1.0.5",
    "underscore": "^1.8.3",
    "url-loader": "^0.6.2",
    "vue": "^2.5.2",
    "vue-hot-reload-api": "^2.2.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.3.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.2",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "zane-calendar": "^2.*.*"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "moment": "^2.17.1"
  }
}
